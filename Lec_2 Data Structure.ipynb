   "source": [
    "# 创建一个 3x3 的二维数组\n",
    "matrix = [\n",
    "    [1, 2, 3],\n",
    "    [4, 5, 6],\n",
    "    [7, 8, 9]\n",
    "]\n",
    "\n",
    "# 外层循环：遍历行\n",
    "for i in range(len(matrix)):\n",
    "    # 内层循环：遍历列\n",
    "    for j in range(len(matrix[i])):\n",
    "        # 打印当前元素\n",
    "        print(matrix[i][j], end=' ')\n",
    "    print()  # 每行输出后换行\n"
   ]
